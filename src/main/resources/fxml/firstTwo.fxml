<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="797.0" prefWidth="980.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="anningtex.controller.FirstTwoController">
    <children>
        <TableView fx:id="tableView" layoutX="101.0" layoutY="119.0" prefHeight="560.0" prefWidth="960.0"
                   AnchorPane.bottomAnchor="227.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                   AnchorPane.topAnchor="10.0">
            <columns>
                <TableColumn fx:id="seq" prefWidth="75.0" text="序号"/>
                <TableColumn fx:id="tcFlowerNo" prefWidth="130.0" text="花型"/>
                <TableColumn fx:id="tcFlowerUrlMini" prefWidth="280.0" text="设计图"/>
                <TableColumn fx:id="tcFlowerFinishedUrl" prefWidth="184.0" text="成品图"/>
                <TableColumn fx:id="oper" prefWidth="129.0" text="操作"/>
                <TableColumn fx:id="photo" prefWidth="129.0" text="其它"/>
            </columns>
        </TableView>
        <ImageView fx:id="ggy" fitHeight="174.0" fitWidth="270.0" layoutX="355.0" layoutY="608.0" pickOnBounds="true"
                   preserveRatio="true" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="355.0"
                   AnchorPane.rightAnchor="355.0" AnchorPane.topAnchor="608.0"/>
        <Text layoutX="136.0" layoutY="602.0" strokeType="OUTSIDE" strokeWidth="0.0" text="挂钩样" textAlignment="CENTER"
              wrappingWidth="135.0">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Text>
        <Button fx:id="btnDelete" layoutX="723.0" layoutY="726.0" mnemonicParsing="false" onAction="#photoGGYDel"
                text="删除">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <Button fx:id="btnPhoto" layoutX="723.0" layoutY="619.0" mnemonicParsing="false" onAction="#photoGGYUp"
                text="其他">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <Button fx:id="btnFresh" layoutX="846.0" layoutY="681.0" mnemonicParsing="false" onAction="#btnFreshList"
                text="整体刷新">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>